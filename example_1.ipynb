    "from twelvedata import TDClient\n",
    "import os\n",
    "\n",
    "td = TDClient(os.getenv('TD_API'))\n",
    "\n",
    "ts = td.time_series(\n",
    "    symbol=\"EUR/USD\",\n",
    "    interval=\"1day\",\n",
    "    outputsize=\"365\"\n",
    ").as_pandas()\n",
    "\n",
    "print(ts)\n"
   ]
